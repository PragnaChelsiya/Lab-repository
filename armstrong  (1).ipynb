   "source": [
    "n=int(input())\n",
    "a=str(n)\n",
    "b=len(a)\n",
    "result=0\n",
    "for i in a:\n",
    "    result+=int(i)**b\n",
    "if result==n:\n",
    "    print(n,\"armstrong number\")\n",
    "else:\n",
    "    print(n,\"not armstrong number\")"
   ]
